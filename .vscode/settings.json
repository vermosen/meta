{
    "cmake.configureArgs": [],
    "cmake.buildDirectory": "${workspaceFolder}/${buildType}",
    "cmake.buildTask": false,
    "cmake.configureSettings": { 
        "CMAKE_INSTALL_PREFIX": "${workspaceFolder}",
        "PYTHON_LOCATION_HINT": "/opt/miniconda/envs/py38/bin",
        "CONAN_BIN_PATH": "/opt/miniconda/envs/py38/bin",
        "CONAN_CHANNEL": "stable",
        "CONAN_USERNAME": "jvermosen",
        "LLVM_TOOLING": false,
        "VERBOSE": true
    },
    "cmake.generator": "Ninja",
    "cmake.environment": {
      "PATH": "/opt/miniconda/envs/py38/bin:${env:PATH}"
